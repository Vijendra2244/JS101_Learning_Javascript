// Store the name, school, grade, section, rollno and the marks scored by the student in 3 subjects
// Print the report card of the student (You can make it look nice by using some keyboard symbols )
// Explore ASCII ART (https://en.wikipedia.org/wiki/ASCII_art (Links to an external site.)) or Text Art (https://fsymbols.com/text-art/ (Links to an external site.)) for some inspiration



let name=" Aman Silawat|";
let School="joy academy |";
let Grade="A✿          |";
let Section="B✿          |";
let Roll ="23          |";
console.log(" ----------------------------")
let Mathematics=78;
let English=85;
let Hindi=95;
console.log("| Name        =" ,name)
console.log("| School Name = ",School)
console.log("| Grade       = ",Grade)
console.log("| Section     = ",Section)
console.log("| Roll No     = ",Roll)
console.log(" ----------------------------")
console.log("                              ")
console.log("✿_________✿ MARKS ✿_________✿")
console.log("                              ")
console.log(" .............................")
console.log("                              ")
console.log("   Subject","        ", "Marks")
console.log("                           ")
console.log("   01) Maths   =    ",Mathematics)
console.log("   02) English =    ",English)
console.log("   03) Hindi   =    ",Hindi)
console.log(" .............................")
